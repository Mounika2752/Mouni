import java.util.*;
public class Main {
	public static void main(String[] args) {
		int i,j,n;
		System.out.println("enter number of rows of stars to be printed");
		Scanner s=new Scanner(System.in);
		n=s.nextInt();
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=n+1-i;j++)
			{
				System.out.print("* ");
			}
			System.out.println();
		}
	}
}







OUTPUT:
enter number of rows of stars to be printed
5
* * * * *
* * * *
* * *
* *
*

