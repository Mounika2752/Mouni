import java.util.*;
class Character
{
	public static void main(String args[])
	{
		int i,j,n;
		char ch='A';
		System.out.println("enter no.of rows to be printed");
		Scanner s=new Scanner(System.in);
		n=s.nextInt();		
		for(i=1;i<=n;i++)
		{
		for(j=1;j<=i;j++)
		{
		System.out.print(ch+ " ");
		ch++;
		}
		System.out.println();
		
		}
	}
}








OUTPUT:
enter no.of rows to be printed
5
A
B C
D E F
G H I J
K L M N O


