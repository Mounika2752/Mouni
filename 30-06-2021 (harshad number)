import java.util.*;
class Sumandproduct
{
	public static void main(String args[]) 
	{
	int n,r,s=0,m=1;
	Scanner Sc=new Scanner(System.in);
	n=Sc.nextInt();
	int k=n;
	while(n>0)
	{
	    r=n%10;
	    n=n/10;
	    s=s+r;
	}
	if(n%s==0)
		System.out.println(k+ " is a harshad number");
		else
		System.out.println(k+ " is not a harshad number");
	}
}






OUTPUT 1:
156
156 is a harsed number

OUTPUT 2:
22
22 is a harsed number




