import java.util.*;
class program2
{
	public static void main(String args[])
	{
		HashMap<String,Integer>x=new HashMap<String,Integer>();
		Scanner sc=new Scanner(System.in);
		int n; 
		String key;
		int value;
		System.out.println("enter numbers elements into hashmap");
		n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
			key=sc.next();
			value=sc.nextInt();
			x.put(key,value);
		}
		System.out.println("enter key to search value");
		String k=sc.next();
		if(x.containsKey(k))
		System.out.print(x.get(k));
		else
		System.out.println("false");
	}
}
	



OUTPUT 1:
enter numbers elements into hashmap
3
sak 5
varun 7
vijay 3
enter key to search value
sak
5



OUTPUT 2:
enter numbers elements into hashmap
4
csdb 4
edsg 6
abcd 8
bhdv 9
enter key to search value
dfrh
false

