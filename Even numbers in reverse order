import java.util.*;
public class Main {
public static void main(String[] args) {
    int n,m;
    System.out.println("enter two numbers ti print even numbers in revrse order");
    Scanner s=new Scanner(System.in);
    m=s.nextInt();
    n=s.nextInt();
    while(m>=n)
    {
	if((m&1)==0)
	 {
		System.out.println(m);
          }
	m--;
     }
   }
}






